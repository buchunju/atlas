OUTPUT_FORMAT("binary")

ENTRY(_start)

SECTIONS{
	
    . = 0x1000;

    .text :  {
        *(.text*)
    }

    .rodata : {
        *(.rodata*)
    }

    .data : {
        *(.data*)
    }

    .bss : {
        bss_begin = .;
        *(COMMON)
        *(.bss*)
        bss_end = .;
    }

    /DISCARD/ : {
        *(*)
    }
}